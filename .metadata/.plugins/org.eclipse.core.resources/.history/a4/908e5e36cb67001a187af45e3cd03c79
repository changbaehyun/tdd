package com.currency.demo;

public class France {

	private int amount;

	public France(int amount) {
		this.amount = amount;
	}

	/*
	 * public void times(int multiplier) { amount *=multiplier; }
	 */
	Dollar times(int multiplier) {
		return new Dollar(amount * multiplier);
	}
	
	public boolean equals(Object object) {
		France dollar = (France) object;
		return amount == dollar.amount;
	}

}
